class Car implements Vehicle {
    
    // your code goes here
    private String make;
    private String color;
    public Car(String color,String make) {
        this.color = color;
        this.make = make;
    }
    String getMake() {
        return make;
    }
    /**
     * @return the color of the vehicle
     */
    String getColor() {
        return color;
    }
}